<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.ahmadrizaldi.traveloka_retrofit.MainActivity">

    <Button
        android:id="@+id/btGet" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Get"/>
    <Button
        android:id="@+id/btInsert" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Insert"/>
    <Button
        android:id="@+id/btUpdate" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Update"/>
    <Button
        android:id="@+id/btDelete" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Delete"/>

    <Button
        android:id="@+id/showData" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Show Data"/>


</LinearLayout>
